<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent"
	android:layout_height="match_parent">

	<ScrollView
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:gravity="center_horizontal">

		<LinearLayout
			android:orientation="vertical"
			android:layout_width="match_parent"
			android:layout_height="match_parent">

			<LinearLayout
				android:orientation="horizontal"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_gravity="center_vertical|center_horizontal">

				<LinearLayout
					android:orientation="vertical"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content">

					<TextView
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="Estudios Realisados"
						android:layout_gravity="center_horizontal"
						android:layout_marginTop="10dp"
						android:textSize="20sp"/>

					<LinearLayout
						android:orientation="horizontal"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_gravity="center_horizontal"
						android:layout_marginTop="10dp">

						<TextView
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:text="Primarios:"/>

						<EditText
							android:layout_width="wrap_content"
							android:ems="10"
							android:layout_height="wrap_content"
							android:editable="false"
							android:id="@+id/Eprima"/>

					</LinearLayout>

					<LinearLayout
						android:orientation="horizontal"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_marginTop="10dp"
						android:layout_gravity="center_horizontal">

						<TextView
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:text="Secundarios:"/>

						<EditText
							android:layout_width="wrap_content"
							android:ems="10"
							android:layout_height="wrap_content"
							android:editable="false"
							android:id="@+id/Esecun"/>

					</LinearLayout>

					<LinearLayout
						android:orientation="horizontal"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_gravity="center_horizontal"
						android:layout_marginTop="10dp">

						<TextView
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:text="Superiores:"/>

						<EditText
							android:layout_width="wrap_content"
							android:ems="10"
							android:layout_height="wrap_content"
							android:editable="false"
							android:id="@+id/Esuperi"/>

					</LinearLayout>

				</LinearLayout>

				<LinearLayout
					android:layout_height="wrap_content"
					android:layout_width="280dp"
					android:orientation="horizontal"/>

				<LinearLayout
					android:orientation="vertical"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content">

					<TextView
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="Cursos"
						android:layout_marginTop="10dp"
						android:textSize="20sp"
						android:layout_gravity="center_vertical|center_horizontal"/>

					<LinearLayout
						android:orientation="horizontal"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_marginTop="15dp"
						android:layout_gravity="center_horizontal">

						<LinearLayout
							android:orientation="vertical"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content">

							<EditText
								android:layout_width="300dp"
								android:ems="10"
								android:layout_height="wrap_content"
								android:editable="false"
								android:id="@+id/cuF1ros1"/>

							<EditText
								android:layout_width="300dp"
								android:ems="10"
								android:layout_height="wrap_content"
								android:layout_marginTop="10dp"
								android:editable="false"
								android:id="@+id/cuF1ros2"/>

							<EditText
								android:layout_width="300dp"
								android:ems="10"
								android:layout_height="wrap_content"
								android:layout_marginTop="10dp"
								android:editable="false"
								android:id="@+id/cuF1ros3"/>

						</LinearLayout>

					</LinearLayout>

				</LinearLayout>

			</LinearLayout>

			<LinearLayout
				android:orientation="horizontal"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_gravity="center_vertical|center_horizontal"
				android:layout_marginTop="46dp">

				<LinearLayout
					android:orientation="vertical"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content">

					<TextView
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="Referencias Laborales"
						android:layout_gravity="center_horizontal"
						android:layout_marginTop="51dp"
						android:textSize="20sp"/>

					<LinearLayout
						android:layout_width="317dp"
						android:layout_height="wrap_content"
						android:layout_gravity="center_horizontal"
						android:layout_marginTop="30dp"
						android:orientation="horizontal">

						<TextView
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:layout_marginRight="82dp"
							android:text="Celular" />

						<TextView
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:layout_marginRight="78dp"
							android:text="Profesion" />

						<TextView
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:text="Nombre" />

					</LinearLayout>

					<LinearLayout
						android:orientation="horizontal"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_marginTop="15dp"
						android:layout_gravity="center_horizontal">

						<LinearLayout
							android:orientation="vertical"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:layout_marginRight="5dp">

							<EditText
								android:id="@+id/RefLN1"
								android:layout_width="135dp"
								android:layout_height="wrap_content"
								android:ems="10"
								android:editable="false" />

							<EditText
								android:id="@+id/RefLN2"
								android:layout_width="135dp"
								android:layout_height="wrap_content"
								android:layout_marginTop="10dp"
								android:ems="10"
								android:editable="false" />

							<EditText
								android:id="@+id/RefLN3"
								android:layout_width="135dp"
								android:layout_height="wrap_content"
								android:layout_marginTop="10dp"
								android:ems="10"
								android:editable="false" />

						</LinearLayout>

						<LinearLayout
							android:layout_width="135dp"
							android:layout_height="wrap_content"
							android:layout_marginRight="5dp"
							android:orientation="vertical">

							<EditText
								android:id="@+id/RefN1"
								android:layout_width="match_parent"
								android:layout_height="wrap_content"
								android:ems="10"
								android:editable="false" />

							<EditText
								android:id="@+id/RefN2"
								android:layout_width="match_parent"
								android:layout_height="wrap_content"
								android:layout_marginTop="10dp"
								android:ems="10"
								android:editable="false" />

							<EditText
								android:id="@+id/RefN3"
								android:layout_width="135dp"
								android:layout_height="wrap_content"
								android:layout_marginTop="10dp"
								android:ems="10"
								android:editable="false" />

						</LinearLayout>

						<LinearLayout
							android:orientation="vertical"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content">

							<EditText
								android:id="@+id/NRe1"
								android:layout_width="135dp"
								android:layout_height="wrap_content"
								android:ems="10"
								android:editable="false" />

							<EditText
								android:id="@+id/NRe2"
								android:layout_width="135dp"
								android:layout_height="wrap_content"
								android:layout_marginTop="10dp"
								android:ems="10"
								android:editable="false" />

							<EditText
								android:id="@+id/NRe3"
								android:layout_width="135dp"
								android:layout_height="wrap_content"
								android:layout_marginTop="10dp"
								android:ems="10"
								android:editable="false" />

						</LinearLayout>






				</LinearLayout>

				</LinearLayout>

				<LinearLayout
					android:layout_width="80dp"
					android:layout_height="wrap_content"
					android:orientation="vertical" />

				<LinearLayout
					android:orientation="vertical"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content">


					<TextView
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="Referencias Familiares"
						android:layout_gravity="center_horizontal"
						android:layout_marginTop="51dp"
						android:textSize="20sp"/>

					<LinearLayout
						android:layout_width="317dp"
						android:layout_height="wrap_content"
						android:layout_gravity="center_horizontal"
						android:layout_marginTop="30dp"
						android:orientation="horizontal">

						<TextView
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:layout_marginRight="82dp"
							android:text="Celular" />

						<TextView
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:layout_marginRight="78dp"
							android:text="Profesion" />

						<TextView
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:text="Nombre" />

					</LinearLayout>

					<LinearLayout
						android:orientation="horizontal"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_marginTop="15dp"
						android:layout_gravity="center_horizontal">

						<LinearLayout
							android:orientation="vertical"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:layout_marginRight="5dp">

							<EditText
								android:id="@+id/CeRF1"
								android:layout_width="135dp"
								android:layout_height="wrap_content"
								android:ems="10"
								android:editable="false" />

							<EditText
								android:id="@+id/CeRF2"
								android:layout_width="135dp"
								android:layout_height="wrap_content"
								android:layout_marginTop="10dp"
								android:ems="10"
								android:editable="false" />

							<EditText
								android:id="@+id/CeRF3"
								android:layout_width="135dp"
								android:layout_height="wrap_content"
								android:layout_marginTop="10dp"
								android:ems="10"
								android:editable="false" />

						</LinearLayout>

						<LinearLayout
							android:layout_width="135dp"
							android:layout_height="wrap_content"
							android:layout_marginRight="5dp"
							android:orientation="vertical">

							<EditText
								android:id="@+id/ReN1"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:ems="10"
								android:editable="false" />

							<EditText
								android:id="@+id/ReN2"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:layout_marginTop="10dp"
								android:ems="10"
								android:editable="false" />

							<EditText
								android:id="@+id/ReN3"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:layout_marginTop="10dp"
								android:ems="10"
								android:editable="false" />

						</LinearLayout>
						<LinearLayout
							android:orientation="vertical"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content">

							<EditText
								android:id="@+id/NRFn1"
								android:layout_width="135dp"
								android:layout_height="wrap_content"
								android:ems="10"
								android:editable="false" />

							<EditText
								android:id="@+id/NRFn2"
								android:layout_width="135dp"
								android:layout_height="wrap_content"
								android:layout_marginTop="10dp"
								android:ems="10"
								android:editable="false"/>

							<EditText
								android:id="@+id/NRFn3"
								android:layout_width="135dp"
								android:layout_height="wrap_content"
								android:layout_marginTop="10dp"
								android:ems="10"
								android:editable="false" />

						</LinearLayout>

					</LinearLayout>

				</LinearLayout>

			</LinearLayout>

			<Button
				android:id="@+id/activityexperienButton1"
				android:layout_width="160dp"
				android:layout_height="55dp"
				android:layout_gravity="center_horizontal"
				android:layout_marginTop="45dp"
				android:background="@drawable/bbutton"
				android:text="Salir"

				android:textColor="#FFFFFF" />

			<LinearLayout
				android:orientation="vertical"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginTop="100dp"/>

		</LinearLayout>

	</ScrollView>

</FrameLayout>

